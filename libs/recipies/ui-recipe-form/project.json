{
  "projectType": "library",
  "sourceRoot": "libs/recipies/ui-recipe-form/src",
  "prefix": "cook-it",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/recipies/ui-recipe-form"],
      "options": {
        "jestConfig": "libs/recipies/ui-recipe-form/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/recipies/ui-recipe-form/**/*.ts",
          "libs/recipies/ui-recipe-form/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:recipes", "type:ui"]
}
