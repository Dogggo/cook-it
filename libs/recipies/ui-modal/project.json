{
  "name": "recipies-ui-modal",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/recipies/ui-modal/src",
  "prefix": "cook-it",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/recipies/ui-modal/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/recipies/ui-modal/**/*.ts",
          "libs/recipies/ui-modal/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:recipes", "type:ui"]
}
